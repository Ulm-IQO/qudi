body {color: #f8f8f2; font-family: sans;}
a {color: #8be9fd;}
.entry {}
.error .message {color: #ff5555}
.warning .message {color: #ffb86c}
.user .message {color: #bd93f9}
.thread .message {color: #ff79c6}
.status .message {color: #50fa7b}
.logExtra {margin-left: 40px;}
.traceback {color: #999; height: 0px;}
.timestamp {color: #f8f8f2;}

